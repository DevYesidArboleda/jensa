  /* Escribenos */
  "flex-layout.col#escribenos-menu": {
    "props": {
      "blockClass": "escribenos",
      "preventVerticalStretch": true
    },
    "children": [
      "vtex.menu@2.x:menu#escribenos"
    ]
  },
  "vtex.menu@2.x:menu#escribenos": {
    "props": {
      "items": [{
        "id": "whatsapp-menu",
        "type": "custom",
        "iconId": null,
        "highlight": false,
        "itemProps": {
          "type": "internal",
          "href": "/contacto",
          "noFollow": false,
          "tagTitle": "ESCRIBENOS",
          "text": "ESCRIBENOS"
        }
      }]
    }
  },
