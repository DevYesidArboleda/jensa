{
  "flex-layout.row#Footer-Xtrategik": {
    "children": ["flex-layout.row#Footer-Xtrategik-CT"],
    "props": {
      "blockClass": "Footer-Xtrategik",
      "fulwidth": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "center",
      "verticalAlign": "middle",
      "paddingTop": 0,
      "paddingBottom": 0
    }
  },
  "flex-layout.row#Footer-Xtrategik-CT": {
    "children": [
      "rich-text#TextoDerechos",
      "rich-text#TextoXtrategik",
      "image#Xtrategik",
      "rich-text#TextoTecnologiaVTEX",
      "image#VTEX"
    ],
    "props": {
      "blockClass": "Footer-Xtrategik-CT",
      "fulwidth": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": false,
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "center",
      "verticalAlign": "middle",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "rich-text#TextoDerechos": {
    "props": {
      "text": "Copyright © 2022 JEN SA. Derechos Reservados. Util.com.co.",
      "blockClass": ["TextoXtrategik","CopyrigthTextoXtrategik"],
      "textAlignment": "LEFT",
      "textPosition": "LEFT"
    }
  },
  "rich-text#TextoXtrategik": {
    "props": {
      "text": "Xtrategik agencia ecommerce",
      "blockClass": "TextoXtrategik",
      "textAlignment": "LEFT",
      "textPosition": "LEFT"
    }
  },
  "image#Xtrategik": {
    "props": {
      "src": "assets/icons/xtrategikIcon.svg",
      "alt": "Xtrategik Agencia Ecommerce",
      "width": "70px",
      "blockClass": "XtrategiK",
      "link": {
        "url": "https://www.xtrategik.com",
        "openNewTab": true,
        "title": "Xtrategik Agencia Ecommerce"
      }
    }
  },
  "image#VTEX": {
    "props": {
      "src": "assets/icons/vtexIcon.svg",
      "alt": "Tecnología VTEX",
      "width": "40px",
      "blockClass": "VTEX",
      "link": {
        "url": "https://www.vtex.com",
        "openNewTab": true,
        "title": "Tecnología VTEX"
      }
    }
  },
  "rich-text#TextoTecnologiaVTEX": {
    "props": {
      "text": "Tecnología",
      "blockClass": "TextoTecnologiaVTEX",
      "textAlignment": "LEFT",
      "textPosition": "LEFT"
    }
  },

  "flex-layout.row#Footer-Xtrategik-Mobile-not": {
