  "flex-layout.row#Footer-Xtrategik-Mini": {
    "children": ["flex-layout.row#Footer-Xtrategik-CT", "flex-layout.col#escribenos-menu"],
    "props": {
      "blockClass": ["Footer-Xtrategik", "footerMini"],
